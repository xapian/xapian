# Make sure aclocal does not include definitions that are not actually
# evaluated.

. ./defs || exit 1

set -e

cat >> configure.in << 'END'
AC_SUBST([POM])
END

mkdir m4
cat >m4/some.m4 <<'EOF'
AC_DEFUN([AM_SOME_MACRO],
[AC_DEFUN([AC_SUBST], [GREPME])])
EOF

$ACLOCAL -I m4
grep m4/some.m4 aclocal.m4 && exit 1
:
