define build-perl-script
$(PERL) -I $(srcdir) -c $(firstword $^)
rm -rf $@
echo "#!" $(PERL) > $@
cat $(firstword $^) >> $@
chmod ugo+rx $@
endef

cvs2cl: $(srcdir)/cvs2cl.pl
	rm -rf $@
	cat $(firstword $^) >> $@
	chmod ugo+rx $@

