	   elif test "X$host" != "X$build" && \
	        { test -x "$bindir/wrong" || test -x "$bindir/wrong"$EXEEXT; }
	   then (exit 77); else (exit $lt_status); fi ) >"$at_stdout" 2>"$at_stderr"
